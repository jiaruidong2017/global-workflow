#! /usr/bin/env bash

########## config.sfcanl ##########
# GFS surface analysis specific

echo "BEGIN: config.sfcanl"

# Get task specific resources
. $EXPDIR/config.resources sfcanl

# Turn off NST in JEDIATMVAR
if [[ "${DO_JEDIATMVAR}" == "YES" ]]; then
   export DONST="NO"
fi

# Turn off SNOW DA for three cycles except 00z
if [[ $SNOWOBSTYPE == "GHCN" && $cyc != "00" ]]; then
   export DO_JEDISNOWDA="NO"
fi

echo "END: config.sfcanl"
